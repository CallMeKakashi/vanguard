{
  "name": "vanguard-dashboard",
  "private": true,
  "version": "1.0.0",
  "description": "Vanguard Tactical Dashboard",
  "author": "Antigravity",
  "type": "module",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "server": "tsx --watch server.ts",
    "start": "concurrently \"npm run server\" \"npm run dev\"",
    "build": "vite build",
    "preview": "vite preview",
    "server:compile": "tsc -p tsconfig.server.json",
    "electron:compile": "tsc -p electron/tsconfig.json && npm run server:compile",
    "electron:dev": "npm run electron:compile && concurrently -k \"cross-env BROWSER=none npm run dev -- --port 5180\" \"wait-on http://localhost:5180 && electron .\"",
    "electron:build": "npm run electron:compile && vite build && electron-builder"
  },
  "build": {
    "appId": "com.vanguard.app",
    "productName": "Vanguard",
    "directories": {
      "output": "dist-electron-build"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*",
      "package.json"
    ],
    "win": {
      "target": "zip",
      "verifyUpdateCodeSignature": false,
      "forceCodeSigning": false
    },
    "linux": {
      "target": "AppImage"
    },
    "mac": {
      "target": "dmg"
    }
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-slot": "^1.2.4",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-router": "^1.157.18",
    "axios": "^1.6.0",
    "class-variance-authority": "^0.7.1",
    "cmdk": "^1.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "framer-motion": "^12.29.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.13.0",
    "recharts": "^3.7.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.1.0",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.24",
    "clsx": "^2.1.1",
    "concurrently": "^8.2.2",
    "cross-env": "^10.1.0",
    "electron": "^40.1.0",
    "electron-builder": "^26.7.0",
    "electron-squirrel-startup": "^1.0.1",
    "lucide-react": "^0.563.0",
    "postcss": "^8.5.6",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^5.0.0",
    "wait-on": "^9.0.3"
  }
}